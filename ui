local player = game.Players.LocalPlayer
wait(0.1)
Workspace[player.Name].Torso:Destroy()
wait(1)
local bool = true


if bool then
    game.Workspace.FallenPartsDestroyHeight = 0 / 0
else
    game.Workspace.FallenPartsDestroyHeight = -500
end

